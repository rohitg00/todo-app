#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rohitghumare/demo-bit/myws/node_modules/.pnpm/vitest@2.1.9_@types+node@20.12.10_jsdom@24.1.3_less@4.2.2_lightningcss@1.29.3_sass@1.86.0_terser@5.39.0/node_modules/vitest/node_modules:/Users/rohitghumare/demo-bit/myws/node_modules/.pnpm/vitest@2.1.9_@types+node@20.12.10_jsdom@24.1.3_less@4.2.2_lightningcss@1.29.3_sass@1.86.0_terser@5.39.0/node_modules:/Users/rohitghumare/demo-bit/myws/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rohitghumare/demo-bit/myws/node_modules/.pnpm/vitest@2.1.9_@types+node@20.12.10_jsdom@24.1.3_less@4.2.2_lightningcss@1.29.3_sass@1.86.0_terser@5.39.0/node_modules/vitest/node_modules:/Users/rohitghumare/demo-bit/myws/node_modules/.pnpm/vitest@2.1.9_@types+node@20.12.10_jsdom@24.1.3_less@4.2.2_lightningcss@1.29.3_sass@1.86.0_terser@5.39.0/node_modules:/Users/rohitghumare/demo-bit/myws/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../vitest/vitest.mjs" "$@"
fi
